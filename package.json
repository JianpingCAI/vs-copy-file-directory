{
  "name": "copy-directory-path",
  "displayName": "copy directory path",
  "description": "A simple extension to copy the directory path of the current file",
  "version": "0.0.2",
  "publisher": "jianping",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "homepage": "https://github.com/JianpingCAI/vs-copy-file-directory/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/JianpingCAI/vs-copy-file-directory.git"
  },
  "bugs": {
    "url": "https://github.com/JianpingCAI/vs-copy-file-directory/issues"
  },
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "copy-file-directory.copyFileDirectory",
        "key": "ctrl+alt+d",
        "mac": "cmd+alt+d",
        "when": "editorTextFocus"
      },
      {
        "command": "copy-file-directory.copyRelativeFileDirectory",
        "key": "ctrl+alt+r",
        "mac": "cmd+alt+r",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "copy-file-directory.copyFileDirectory",
        "title": "Copy File Directory"
      } ,
      {
        "command": "copy-file-directory.copyRelativeFileDirectory",
        "title": "Copy File Relative Directory"
      }     
    ],
    "menus": {
      "editor/title/context": [
        {
          "command": "copy-file-directory.copyFileDirectory",
          "group": "1_cutcopypaste@10"
        },
        {
          "command": "copy-file-directory.copyRelativeFileDirectory",
          "group": "1_cutcopypaste@11"
        }
      ],
      "explorer/context": [
        {
          "command": "copy-file-directory.copyFileDirectory",
          "group": "6_copypath@100"
        },
        {
          "command": "copy-file-directory.copyRelativeFileDirectory",
          "group": "6_copypath@101"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}
